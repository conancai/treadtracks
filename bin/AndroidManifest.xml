<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.treadtracksproto"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.treadtracksproto.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.treadtracksproto.StatsPage"
            android:label="@string/title_activity_stats_page" >
        </activity>
        <activity
            android:name="com.example.treadtracksproto.CalibrationPage"
            android:label="@string/title_activity_calibration_page" >
        </activity>
        <activity
            android:name="com.example.treadtracksproto.RunActivity"
            android:label="@string/title_activity_calibration_page" >
        </activity>
        <activity
            android:name="com.example.treadtracksproto.RunningActivity"
            android:label="@string/title_activity_running" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
